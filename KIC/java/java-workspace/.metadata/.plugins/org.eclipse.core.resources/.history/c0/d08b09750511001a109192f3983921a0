	void selectionSort(int[] list, int n)
	{

		int least, temp;

		for (int i = 0; i < n - 1; ++i) {
			least = i;
			for (int j = i + 1; j < n; ++j)
				if (list[j] < list[least]) {
					least = j;
					temp = list[i];
					list[i] = list[least];
					list[least] = temp;
				}
		}
	}

}
