	<script type="text/javascript">
		var arr1 = [];
		var arr2 = [273, 32, 103, 57, 52];
		
		console.log(arr1);
		console.log(arr2);
		
		document.write(arr1);
		document.write(arr2);
		

		console.log(arr1[0]);
		console.log(arr2[0]);
		console.log(arr2[1]);
		
		arr1[0] = 10;
		arr1[1] = 20;
		arr1[3] = 30;

		console.log(arr1);
		console.log(arr1.length);
		
		// undefined 빠짐
		// index가 나옴 : 자바스크립트
		for(var i in arr1) {
			console.log( i + ' : ' + arr1[i]);
		}
		
		// Array 객체
		var arr3 = Array(10, 20, 30, 40);
		console.log( arr3 );
		
		console.log( arr3[arr3.length - 1] );
		
		
		
		
	</script>
